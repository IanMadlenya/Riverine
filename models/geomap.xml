<?xml version="1.0" encoding="ISO-8859-15"?>

<model>
    <data>
        <file value="models/data/geomap.csv"></file>
        <column_names value="True"></column_names>
        	<parameters>
        	     <param id="country" label="Country" value="xaxis"></param>
        	     <param id="continent" label="Continent" value="null"></param>             
        	     <param id="population" label="Population" value="yaxis"></param>
        	     <param id="GDPcap" label="GDP Cap" value="yaxis"></param>
        	     <param id="GERD" label="GERD" value="ID"></param>
        	     <param id="growth" label="Growth" value="yaxis"></param>
        	</parameters>
        	<categories>
        	     <category value="population" label="Population"></category>	
        	     <category value="GDPcap" label="GDP cap"></category>	
        	     <category value="GERD" label="GERD"></category>	
        	     <category value="growth" label="Growth"></category>	
        	</categories>
    </data>
    <visualizer>
        <view type="geomap"></view>
    </visualizer>
</model>

